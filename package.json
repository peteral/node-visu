{
  "name": "node-visu",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "scripts": {
    "start-server": "node ./bin/www",
    "build-all": "npm run-script build-libs && npm run-script build-frontend && npm run-script build-pictures && npm run-script build-emes-runtime && npm run-script build-emes-editor",
    "build-libs": "copyfiles -f ./node_modules/react/dist/react.js ./node_modules/react-dom/dist/react-dom.js ./node_modules/alt/dist/alt.js ./node_modules/socket.io-client/socket.io.js ./public/javascripts",
    "build-frontend": "webpack --config webpack-frontend.config.js",
    "build-emes-runtime": "webpack --config webpack-emes-runtime.config.js",
    "build-emes-editor": "webpack --config webpack-emes-editor.config.js",
    "build-pictures": "webpack --config webpack-pictures.config.js"
  },
  "dependencies": {
    "ag-grid": "^4.2.5",
    "ag-grid-react": "^4.2.0",
    "alt": "^0.18.4",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "flux": "^2.1.1",
    "jade": "~1.11.0",
    "lodash": "^4.12.0",
    "morgan": "~1.6.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-grid-layout": "^0.12.4",
    "react-highlight": "^0.8.0",
    "react-skylight": "^0.4.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "^12.0.2",
    "react-addons-test-utils": "^15.0.2",
    "webpack": "^1.13.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  }
}
